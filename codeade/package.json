{
  "name": "codeade",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier . -c",
    "format:fix": "prettier . -w",
    "prepare": "husky"
  },

"lint-staged": {
    "*.{ts,tsx,js,jsx, css}": [
      "eslint --fix",
      "prettier --write",
      "echo 'Lint-staged finished'"
    ]
  },

  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "^5"
  }
}
